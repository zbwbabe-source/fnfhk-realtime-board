# Snowflake Configuration
SNOWFLAKE_ACCOUNT=your-snowflake-account
SNOWFLAKE_USERNAME=your-username
SNOWFLAKE_PASSWORD=your-password
SNOWFLAKE_DATABASE=FNF
SNOWFLAKE_SCHEMA=SAP_FNF
SNOWFLAKE_WAREHOUSE=your-warehouse
SNOWFLAKE_ROLE=your-role

# Vercel KV (Redis) Configuration (for Section3 snapshot cache)
KV_REST_API_URL=https://your-kv-endpoint.upstash.io
KV_REST_API_TOKEN=your-kv-rest-api-token

# Section3 Cron Configuration
SECTION3_SNAPSHOT_DAYS=1
SECTION3_CRON_PARALLEL=0

# Cron Job Protection
CRON_SECRET=your-secure-random-string

# OpenAI API Key (for insights)
OPENAI_API_KEY=your-openai-api-key

# Next.js Configuration
NODE_ENV=development
